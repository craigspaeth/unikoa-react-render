{
  "name": "unikoa-react-render",
  "version": "0.0.5",
  "description": "Unikoa middleware for rendering React apps universally",
  "main": "index.js",
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "scripts": {
    "test": "mocha -r should && standard",
    "bundle": "browserify -t babelify example/client.js > example/bundle.js",
    "example": "npm run bundle && node -r babel-core/register ./example/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigspaeth/unikoa-react-render.git"
  },
  "keywords": [
    "koa",
    "unikoa",
    "react",
    "universal",
    "isomorphic",
    "render"
  ],
  "author": "Craig Spaeth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/craigspaeth/unikoa-react-render/issues"
  },
  "homepage": "https://github.com/craigspaeth/unikoa-react-render#readme",
  "peerDependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.11.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "koa": "^2.0.0-alpha.4",
    "koa-static": "^3.0.0",
    "mocha": "^3.0.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "should": "^10.0.0",
    "standard": "^7.1.2",
    "superagent": "^2.1.0",
    "unikoa": "0.0.1"
  }
}
